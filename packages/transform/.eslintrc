{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["jest"],
  "rules": {
    "radix": "off",
    "arrow-parens": "off",
    "prefer-template": "off"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "jest/globals": true
  },
  "globals": {
    "FileReader": true,
    "DOMException": true,
    "fetch": true,
    "ace": true
  }
}
